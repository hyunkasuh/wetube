extends layouts/main
include mixins/videoBlock
block content
    .search__header
        h4 Searching for: #{searchingBy}
    .search__videos
        if videos.length === 0
            h5 No Videos Found
        each item in videos
            +videoBlock ({
                fileUrl: item.fileUrl,
                title: item.title,
                views: item.views,
                id: item.id
            })